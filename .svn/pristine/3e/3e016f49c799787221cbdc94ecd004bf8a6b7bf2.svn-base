.clear {
	clear: both;
}

.user_row {
	margin-top: 20px;
}

.user_form {
	margin-left: 30px;
}

.user_add_btn {
	color: #fff !important;
	background: #444C63 !important;
	margin-left: 100px;
}

.role_name_margin {
	margin-left: 10px;
}

.per_add_btn {
	color: #fff !important;
	background: #444C63 !important;
	margin-left: 130px;
}

.menu_add_btn {
	color: #fff !important;
	background: #444C63 !important;
	margin-left: 150px;
}

.role_add_btn {
	color: #fff !important;
	background: #444C63 !important;
}

.clear_padding {
	padding-left: 8px !important;
}

.user_search_btn {
	color: #fff !important;
	background: #444C63 !important;
}

.user_role {
	margin-left: 10px;
}

.user_select {
	background: #3B4252;
	border: 1px solid #fff;
	color: #fff;
}

.btn_update {
	background: #4AB17E;
	border: 1px solid #4AB17E;
}

.btn_update:hover {
	background: #88b49e;
}

.btn_delete {
	background: #FF8393;
	border: 1px solid #FF8393;
}

.btn_delete:hover {
	background: #fdbac2;
}

.add_systemusers_form_title {
	min-width: 170px;
	height: 30px;
	border-left: 4px solid #5ce59d;
	margin: 22px 0 28px 42px;
	font-size: 22px;
	color: #fff;
	padding-left: 12px;
}

.form_right_style {
	position: relative;
	min-width: 242px;
	height: 28px;
	border-radius: 4px;
	padding-left: 12px;
	background: #fff;
	color: #72727b;
	font-size: 12px;
	padding-right: 22px;
}

.form_right_small {
	display: inline-block;
}

.form_right_red {
	color: #ff5e66;
	width: 22px;
	text-align: center;
}

.form_right_font {
	color: #c2c2c2;
}

.btn_cancel {
	background: #444C63;
	color: #fff;
	border: 1px solid #fff;
}

.btn_cancel:hover {
	background: #b0b0b0;
	color: #fff;
	border: 1px solid #fff;
}

.update_data_dialog {
	background: #444C63 !important;
	color: #fff;
}

.modal_footer_center {
	text-align: center !important;
}

.modal_dialog_width {
	width: 30% !important;
	margin: 30px auto;
	margin-top: 4%;
}

.user_group_sex {
	position: relative;
	width: 66%;
	margin-left: 30px;
}

.cut_title {
	width: 100%;
	min-height: 30px;
	margin-top: 10px;
	border-top: 1px solid #5B637A;
	font-size: 18px;
	color: #fff;
	padding: 10px 0 0 32px;
}

.user_group_content {
	width: 100%;
	min-height: 330px;
	box-sizing: border-box;
}

.help_imgs {
	margin-left: 5px;
}

.cut_right {
	width: 1px;
	background: #5B637A;
	height: 270px;
	margin-top: 10px;
	float: left;
}

.user_group_cut {
	width: 49.9%;
	height: 100%;
}

.user_group {
	width: 100%;
	height: 100%;
}

.user_group_charts {
	clear: both;
	width: 94%;
	height: 270px;
	box-sizing: border-box;
	margin-left: 32px;
}

.instruction_dialog {
	width: 300px;
	padding: 10px;
	margin-left: 10px;
	background: #beceeb;
	border-radius: 10px;
	filter: alpha(opacity = 40);
	opacity: 0.4; /* For IE8 and earlier */
}